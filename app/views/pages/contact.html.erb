      <!-- start: Contact Form -->
      <div class="col-sm-4">
        <div class="title"><h3>Contact Form</h3></div>

        <!-- start: Contact Form -->
        <div id="contact-form">
          <form method="post" action="">
            <fieldset>

              <div class="form-group">
                <label for="name">Name:</label>
                <input type="text" class="form-control" id="name" name="name">
              </div>

              <div class="form-group">
                <label for="email">Email:</label>
                <input type="email" class="form-control" id="email" name="email">
              </div>

              <div class="form-group">
                <label for="www">WWW:</label>
                <input type="text" class="form-control" id="www" name="www">
              </div>

              <div class="form-group">
                <label for="message"><span>Message:</span></label>
                <textarea class="form-control" id="message" name="message" rows="7"></textarea>
              </div>

              <div class="actions">
                <a href="contact.html#" class="btn btn-primary" id="send">Send message</a>
              </div>
            </fieldset>
          </form>
        </div>
        <!-- end: Contact Form -->
      </div>
