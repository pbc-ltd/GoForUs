<!DOCTYPE html>
<html lang='en'>
  <head>
    <%= csrf_meta_tags %>
    <%= render 'layouts/meta_tags' %>

    <%= stylesheet_link_tag  'bootstrap1' %>
    <%= stylesheet_link_tag  'style' %>
    <%= stylesheet_link_tag  'parallax-slider' %>

    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
      <%= javascript_include_tag 'respond.min' %>
    <![endif]-->

    <link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Droid+Sans:400,700">
    <link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Droid+Serif">
    <link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Boogaloo">
    <link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Economica:700,400italic">
  </head>

  <body>
    <%= render 'layouts/navigation' %>

    <% unless yield(:page_title).empty? %>
      <!-- start: Page Title-->
      <div id="page-title">
        <div id="page-title-inner">
          <div class="container">
            <h2><%= yield :page_title %></h2>
          </div>
        </div>
      </div>
      <!-- end: Page Title-->
    <% end %>

    <%= yield %>

    <%= render 'layouts/footer' %>

    <!-- start: Java Script -->
    <!-- Placed at the end of the document so the pages load faster -->
    <%= javascript_include_tag 'jquery-1.9.1.min' %>
    <%= javascript_include_tag 'bootstrap' %>
    <%= javascript_include_tag 'jquery.isotope.min' %>
    <%= javascript_include_tag 'jquery.imagesloaded' %>
    <%= javascript_include_tag 'flexslider' %>
    <%= javascript_include_tag 'carousel' %>
    <%= javascript_include_tag 'jquery.cslider' %>
    <%= javascript_include_tag 'slider' %>
    <%= javascript_include_tag 'jquery.fancybox' %>

    <%= javascript_include_tag 'excanvas' %>
    <%= javascript_include_tag 'jquery.flot' %>
    <%= javascript_include_tag 'jquery.flot.pie.min' %>
    <%= javascript_include_tag 'jquery.flot.stack' %>
    <%= javascript_include_tag 'jquery.flot.resize.min' %>

    <%= javascript_include_tag 'custom' %>

    <%= javascript_include_tag 'jquery_ujs' %>
    <!-- end: Java Script -->

    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
         (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
           m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
             })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
       ga('create', 'UA-75225239-1', 'auto');
       ga('send', 'pageview');
    </script>
  </body>
</html>
