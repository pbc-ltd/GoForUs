<title><%= meta_title %></title>

<meta name="viewport" content="<%= meta_viewport %>">
<meta name="googlebot" content="<%= meta_googlebot %>" />
<meta name="description" content="<%= meta_description  %>" />
<meta name="keywords" content="<%= meta_keywords %>" />
<meta name="author" content="<%= meta_author %>" />

<!-- start: Facebook Open Graph -->
<meta property="og:title" content="<%= meta_title %>"/>
<meta property="og:description" content="<%= meta_description %>"/>
<meta property="og:type" content="<%= meta_og_type %>"/>
<meta property="og:locale" content="<%= meta_og_locale %>"/>
<meta property="og:site_name" content="<%= meta_og_site_name %>"/>
<meta property="og:url" content="<%= current_url %>"/>

<meta property="og:image" content="<%= meta_og_image %>"/>
<meta property="og:image:type" content="<%= meta_og_image_type %>"/>
<meta property="og:image:width" content="<%= meta_og_image_width %>"/>
<meta property="og:image:height" content="<%= meta_og_image_height %>"/>
<!-- end: Facebook Open Graph -->
